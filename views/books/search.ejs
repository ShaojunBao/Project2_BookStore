<%- include('../partials/header') %>

<h1>Search Results for '<%= query %>'</h1>

<div class="books">
  <% books.forEach(function(book) { %>
    <div class="book">
      <img src="<%= book.imagePath %>" alt="<%= book.title %> Cover Image">
      <h3><%= book.title %></h3>
      <!-- other book details -->
      <a href="/books/<%= book._id %>">Details</a>
    </div>
  <% }) %>
</div>

<%- include('../partials/footer') %>